{
    "name": "vipx/bot-detect-bundle",
    "type": "symfony-bundle",
    "description": "Symfony VipxBotDetectBundle",
    "keywords": ["Security", "Bot"],
    "homepage": "http://lennerd.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Hildebrandt",
            "email": "hello@lennerd.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:Stockopedia/vipx-bot-detect.git"
        }
    ],
    "require": {
        "php": "^5.6|7.*",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0",
        "symfony/security-bundle": "^2.8|^3.0|^4.0",
        "stockopedia/bot-detect": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.5",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": { "Vipx\\BotDetectBundle\\": "./" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
